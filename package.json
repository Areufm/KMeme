    "prettier": "^3.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "dependencies": {
    "clipboard": "^2.0.11",
    "dayjs": "^1.11.13",
    "mockjs": "^1.1.0"
